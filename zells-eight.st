Object subclass: #ZDish	instanceVariableNames: 'culture'	classVariableNames: ''	poolDictionaries: ''	category: 'zells-eight'!!ZDish methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:26'!initialize	super initialize.	culture := Set new.! !!ZDish methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:06'!put: aZell	culture add: (aZell containedIn: self).	^ aZell.! !!ZDish methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:08'!remove: aZell 	culture remove: aZell ifAbsent: [].! !!ZDish methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:26'!transmit: aSignal	culture do: [:z|z receive: aSignal]! !Object subclass: #Zell	instanceVariableNames: 'zContainer'	classVariableNames: ''	poolDictionaries: ''	category: 'zells-eight'!!Zell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:03'!containedIn: aContainer	zContainer := aContainer.! !!Zell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:06'!destroy	zContainer remove: self.! !!Zell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:24'!receive: aSignal! !Zell subclass: #LoggingZell	instanceVariableNames: 'myName'	classVariableNames: ''	poolDictionaries: ''	category: 'zells-eight'!!LoggingZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:05'!cloneTo: clonedName	^ zContainer put: (LoggingZell named: clonedName).! !!LoggingZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:26'!name: aString	myName := aString.! !!LoggingZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:25'!receive: aSignal	Transcript show: myName; show: ' received: '; show: aSignal; cr.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LoggingZell class	instanceVariableNames: ''!!LoggingZell class methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 15:26'!named: aString	^ LoggingZell new		name: aString;		yourself.! !Zell subclass: #ProtoZell	instanceVariableNames: 'inhabitants'	classVariableNames: ''	poolDictionaries: ''	category: 'zells-eight'!!ProtoZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:24'!initialize	super initialize.	inhabitants := Set new.! !!ProtoZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:25'!put: aZell	inhabitants add: aZell.	^ aZell.! !!ProtoZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:25'!remove: aZell	inhabitants remove: aZell.! !!ProtoZell methodsFor: 'as yet unclassified' stamp: 'NM 6/13/2018 16:25'!transmit: aSignal	zContainer transmit: aSignal.! !